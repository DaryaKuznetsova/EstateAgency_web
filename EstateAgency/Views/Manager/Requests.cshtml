@model IEnumerable<EstateAgency.ViewModel.EstateObjectViewModel>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutManager.cshtml";
}

<h2>Активные заявки</h2>

<table class="table">
    <tr>
        <th>

        </th>
        <th>
            Описание
        </th>
        <th>

        </th>
        <th>
            Цена
        </th>
        <th>
            Площадь
        </th>
        <th>
            Адрес
        </th>
    </tr>

    @foreach (var e in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(m => e.Id)
        </td>
        <td>
            @Html.DisplayFor(m => e.RealtyType)
        </td>
        <td>
            @Html.DisplayFor(m => e.TradeType)
        </td>
        <td>
            @Html.DisplayFor(m => e.Price)
        </td>
        <td>
            @Html.DisplayFor(m => e.Area)
        </td>
        <td>
            @Html.DisplayFor(m => e.Address)
        </td>
        <td>
            @Html.ActionLink("Рассмотреть заявку", "ViewRequest", new { id = e.Id })
        </td>
    </tr>
    }

</table>
@Html.ActionLink("Назад", "Index")





