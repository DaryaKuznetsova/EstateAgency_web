@model IEnumerable<EstateAgency.ViewModels.TradeRequestViewModel>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutManager.cshtml";
}

<h2>Каталог объектов недвижимости</h2>


<table class="table">
    <tr>
        <th>
            Id
        </th>
        <th>
            Date
        </th>
        <th>
            Price
        </th>
        <th>
            Area
        </th>
        <th>
            Address
        </th>
    </tr>

    @foreach (EstateAgency.ViewModels.TradeRequestViewModel e in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(m => e.Date)
        </td>
        <td>
            @Html.DisplayFor(m => e.EstateObjectId)
        </td>
        <td>
            @Html.DisplayFor(m => e.Price)
        </td>
        <td>
            @Html.DisplayFor(m => e.Area)
        </td>
        <td>
            @Html.DisplayFor(m => e.Address)
        </td>
        <td>
            @Html.ActionLink("Подробнее", "TradeInfo", new { id = e.EstateObjectId })
        </td>
    </tr>
    }

</table>
@Html.ActionLink("Назад", "Index")






