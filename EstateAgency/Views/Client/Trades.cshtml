@model IEnumerable<EstateAgency.EstateObject>


@{
    ViewBag.Title = "Trades";
    Layout = "~/Views/Shared/_LayoutClient.cshtml";
}

<h2>Мои Сделки</h2>


<table class="table">
    <tr>
        <th>
            Id
        </th>
        <th>
            Price
        </th>
        <th>
            Area
        </th>
        <th>
            Address
        </th>
    </tr>

    @foreach (EstateAgency.EstateObject e in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(m => e.Id)
            </td>
            <td>
                @Html.DisplayFor(m => e.Price)
            </td>
            <td>
                @Html.DisplayFor(m => e.Area)
            </td>
            <td>
                @Html.DisplayFor(m => e.Address)
            </td>
            <td>
                @Html.ActionLink("Подробнее", "ViewTrade", new { id = e.Id })
            </td>
        </tr>
    }

</table>
@Html.ActionLink("Назад", "Index")





